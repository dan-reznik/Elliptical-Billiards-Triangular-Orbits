Cell[CellGroupData[{

Cell["\<\
What is the \"a\" such that the line connecting foci goes thru a corner \
(i.e., it is collinear -- vanishing determinant)\
\>", "Subsubsection",
 CellChangeTimes->{{3.7928643615058956`*^9, 3.7928643833317127`*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"xs", "=", 
      RowBox[{"xsol", "[", "a", "]"}]}], ",", "c", ",", "fl", ",", "ft", ",", 
     "det"}], "}"}], ",", "\n", 
   RowBox[{
    RowBox[{"c", "=", 
     RowBox[{"Sqrt", "[", 
      RowBox[{
       RowBox[{"a", "^", "2"}], "-", "1"}], "]"}]}], ";", "\n", 
    RowBox[{"fl", "=", 
     RowBox[{"{", 
      RowBox[{"c", ",", "0", ",", "1"}], "}"}]}], ";", "\n", 
    RowBox[{"ft", "=", 
     RowBox[{"{", 
      RowBox[{"0", ",", "c", ",", "1"}], "}"}]}], ";", "\n", 
    RowBox[{"det", "=", 
     RowBox[{"Det", "[", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"xs", ",", "xs", ",", "1"}], "}"}], ",", "fl", ",", "ft"}], 
       "}"}], "]"}]}], ";", "\n", 
    RowBox[{"a", "/.", 
     RowBox[{"Solve", "[", 
      RowBox[{
       RowBox[{"det", "==", "0"}], ",", "a"}], "]"}]}]}]}], "]"}]], "Input",
 ExpressionUUID -> "51c087a0-3933-4a78-ace6-a62f4e33cef8"],

Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
   RowBox[{"-", "1"}], ",", "1", ",", 
   RowBox[{"\[ImaginaryI]", " ", 
    SqrtBox[
     RowBox[{
      RowBox[{"-", "2"}], "+", 
      SqrtBox["5"]}]]}], ",", 
   SqrtBox[
    RowBox[{"2", "+", 
     SqrtBox["5"]}]]}], "}"}]], "Output",
 CellChangeTimes->{3.7928643493804207`*^9},
 ExpressionUUID -> "bacc6e5e-7955-444c-b0c3-cae2308158a0"]
}, Open  ]],


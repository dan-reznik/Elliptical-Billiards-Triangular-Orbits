Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"drawTwoElls", "[", "a_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"ell1", ",", "ell2", ",", 
       RowBox[{"a2", "=", 
        RowBox[{"a", "*", "a"}]}], ",", "rp", ",", "gr", ",", "xs", ",", 
       "ell3", ",", "r", ",", "c", ",", "fs"}], "}"}], ",", "\n", 
     RowBox[{
      RowBox[{"c", "=", 
       RowBox[{"Sqrt", "[", 
        RowBox[{
         RowBox[{"a", "^", "2"}], "-", "1"}], "]"}]}], ";", "\n", 
      RowBox[{"fs", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "c"}], ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"c", ",", "0"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", 
           RowBox[{"-", "c"}]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"0", ",", "c"}], "}"}]}], "}"}]}], ";", "\n", 
      RowBox[{"ell1", "=", 
       RowBox[{"{", 
        RowBox[{"Blue", ",", 
         RowBox[{"Circle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"a", ",", "1"}], "}"}]}], "]"}]}], "}"}]}], ";", "\n", 
      RowBox[{"ell2", "=", 
       RowBox[{"{", 
        RowBox[{"Blue", ",", 
         RowBox[{"Circle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "0"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"1", ",", "a"}], "}"}]}], "]"}]}], "}"}]}], ";", "\n", 
      RowBox[{"r", "=", 
       RowBox[{"ellCircRad", "[", "a", "]"}]}], ";", "\n", 
      RowBox[{"ell3", "=", 
       RowBox[{"{", 
        RowBox[{"Red", ",", 
         RowBox[{"Circle", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"0", ",", "0"}], "}"}], ",", "r"}], "]"}]}], "}"}]}], ";",
       "\n", 
      RowBox[{"rp", "=", 
       RowBox[{"RegionPlot", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"x", "^", "2"}], "+", 
             RowBox[{"a2", " ", 
              RowBox[{"y", "^", "2"}]}]}], "\[LessEqual]", "a2"}], ")"}], "&&", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"a2", " ", 
              RowBox[{"x", "^", "2"}]}], "+", 
             RowBox[{"y", "^", "2"}]}], "\[LessEqual]", "a2"}], ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", 
           RowBox[{"-", "a"}], ",", "a"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"y", ",", 
           RowBox[{"-", "a"}], ",", "a"}], "}"}]}], "]"}]}], ";", "\n", 
      RowBox[{"xs", "=", 
       RowBox[{"xsol", "[", "a", "]"}]}], ";", "\n", 
      RowBox[{"gr", "=", 
       RowBox[{"Graphics", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"PointSize", "@", "Large"}], ",", "\n", "ell1", ",", "ell2",
           ",", "ell3", ",", 
          RowBox[{"Point", "@", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"xs", ",", "xs"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"-", "xs"}], ",", "xs"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"-", "xs"}], ",", 
               RowBox[{"-", "xs"}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"xs", ",", 
               RowBox[{"-", "xs"}]}], "}"}]}], "}"}]}], ",", "Blue", ",", 
          RowBox[{"Point", "@", "fs"}], ",", "Dashed", ",", 
          RowBox[{"Line", "@", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"fs", "[", 
              RowBox[{"[", "2", "]"}], "]"}], ",", 
             RowBox[{"fs", "[", 
              RowBox[{"[", "4", "]"}], "]"}]}], "}"}]}]}], "}"}], "]"}]}], 
      ";", "\n", 
      RowBox[{"Show", "[", 
       RowBox[{"{", 
        RowBox[{"rp", ",", "gr"}], "}"}], "]"}]}]}], "]"}]}], ";"}]], "Input",
